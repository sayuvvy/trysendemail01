<div class="jumbotron">
    <h4 class="font-weight-bolder">Compose New Email</h4>
<div class="row">
    <div class="col-6">
        <div class="form">
            <div class="form-group">
                <label for="emailto">EmailTo</label>
                <input type="text" name="emailto" placeholder="Enter email to" data-role="tagsinput">
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-6">
        <div class="form">
            <div class="form-group">
                <label for="emailcc">Email CC</label>
                <input type="text" name="emailcc" placeholder="Enter email cc" data-role="tagsinput">
            </div>
        </div>
    </div>
</div>

<div class="form">
    <div class="form-group">
        <label for="subject">Subject</label>
        <input type="text" class="form-control" name="subject" placeholder="Enter subject">
    </div>
</div>
<div class="summernote" id="email_template">

</div>
    <a class="btn btn-primary btn-lg" href="#" role="button" id="send">Send</a>
</div>